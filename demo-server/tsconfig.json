{
  "compilerOptions": {
    "baseUrl": "./",
    "module": "commonjs",
    "declaration": true,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "incremental": true,
    "lib": [
      "es2019"
    ],
    "outDir": "./dist",
    "paths": {},
    "removeComments": true,
    "sourceMap": true,
    "target": "es2019"
  },
  "include": ["src/*.ts"],
  "exclude": [
    "dist",
    "node_modules"
  ],
}
